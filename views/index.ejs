<!DOCTYPE html>
<html lang="en">
    <%- include ('./partials/header.ejs')%>
<body>
    <%- include ('./partials/nav.ejs')%>
    <div class="blogs content">
        <h2>All Blogs</h2>
        <% if(blogs.length>0){ %>
            <% blogs.forEach(blog=>{%>
                <h3><%=blog.title %> </h3>
                <p><%=blog.snippet%></p>
            <% })%>
        <% } %>
    </div>
</body>
</html>